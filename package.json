{
  "name": "teamwork",
  "version": "1.0.0",
  "description": "Teamwork is an ​internal social network for employees of an organization. The goal of this application is to facilitate more interaction between colleagues and promote team bonding. ",
  "main": "app.js",
  "scripts": {
    "test": "babel-node mocha ./server/test/**/*.test.js --exit --timeout 20000",
    "try-test": "mocha ./server/test/* --compiler js:babel-core/register --timeout 10000 --exit",
    "start": "nodemon --exec babel-node ./server/server.js",
    "migration": "babel-node ./server/models/db/migrations.js",
    "seed": "babel-node ./server/models/db/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olumidae/Teamwork.git"
  },
  "author": "Omitiran Olumide",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olumidae/Teamwork/issues"
  },
  "homepage": "https://github.com/olumidae/Teamwork#readme",
  "dependencies": {
    "@babel/register": "^7.4.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "nyc": "^14.1.1",
    "pg": "^7.11.0"
  },
  "devDependencies": {
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.0"
  }
